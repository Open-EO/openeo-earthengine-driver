{
  "name": "@openeo/earthengine-driver",
  "version": "1.0.0-alpha.1",
  "stac_version": "0.9.0",
  "description": "An openEO driver for Google Earth Engine.",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js --trace-warnings",
    "up": "forever start -a -l forever.eelog -o out.eelog -e err.eelog server.js",
    "down": "forever stop server.js",
    "test": "jest --env=node",
    "debug": "node --nolazy --inspect-brk=9229 server.js"
  },
  "author": "Matthias Mohr",
  "license": "Apache-2.0",
  "homepage": "http://openeo.org",
  "bugs": {
    "url": "https://github.com/Open-EO/openeo-earthengine-editor/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Open-EO/openeo-earthengine-editor.git"
  },
  "devDependencies": {
    "forever": "^2.0.0",
    "jest": "^25.1.0",
    "jest-html-reporter": "^2.5.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^3.0.2",
    "@google/earthengine": "^0.1.213",
    "@openeo/js-commons": "^1.0.0-rc.2",
    "@openeo/js-processgraphs": "^1.0.0-alpha.3",
    "ajv": "^6.10.0",
    "axios": "^0.19.0",
    "check-disk-space": "^2.1.0",
    "fs-extra": "^8.0.1",
    "nedb": "^1.8.0",
    "object-hash": "^1.3.1",
    "proj4": "^2.6.0",
    "restify": "^8.3.3"
  },
  "nodemonConfig": {
    "ignore": [
      "storage/*"
    ]
  }
}
